{
    "name": "rezzza/model-violation-logger-bundle",
    "description": "Log violations from a model.",
    "keywords": ["log", "model", "symfony", "bundle"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/rezzza/ModelViolationLoggerBundle",
    "license": "MIT",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/rezzza/ModelViolationLoggerBundle/contributors"
    }],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "mageekguy/atoum": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "autoload":     {
        "psr-0": { "Rezzza\\ModelViolationLoggerBundle": "" }
    },
    "target-dir": "Rezzza/ModelViolationLoggerBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
